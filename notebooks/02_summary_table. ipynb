    "import numpy as np\n",
    "\n",
    "# Replace inf, -inf with None (so they become NULL in MySQL)\n",
    "summary_table = summary_table.replace([np.inf, -np.inf], np.nan)\n",
    "\n",
    "# Drop rows if needed (optional, if you donâ€™t want NULLs)\n",
    "# summary_table = summary_table.dropna()\n",
    "\n",
    "# Now save to MySQL\n",
    "summary_table.to_sql(\n",
    "    'summary_table', \n",
    "    con=engine, \n",
    "    if_exists='replace', \n",
    "    index=False,\n",
    "    chunksize=5000,     # to speed up\n",
    "    method='multi'      # batches inserts\n",
    ")\n"
