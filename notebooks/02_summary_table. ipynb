    "freight_summary=pd.read_sql(\"\"\"SELECT VendorNumber,SUM(Freight) AS freight_cost \n",
    "   FROM vendor_invoice\n",
    "   GROUP BY VendorNumber\"\"\", \n",
    "     engine)"
   ]
